
{
    "name": "kewltab",
    "description": "the kewlest new tab of them all!",
    "version": "2.0",
    "manifest_version": 3,
    "chrome_url_overrides": { "newtab": "index.html" },
    "permissions": ["storage"],
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
}